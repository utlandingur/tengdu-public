{
  "name": "tengdu",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo run:web",
    "upgrade:tamagui": "$npm_execpath up '*tamagui*' '@tamagui/*'",
    "predeploy": "expo export -p web",
    "deploy-hosting": "npm run predeploy && firebase deploy --only hosting:tengdu",
    "storybook-generate": "sb-rn-get-stories",
    "storybook-watch": "sb-rn-watcher",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "eas-build-pre-install": "node pre-install.js",
    "eas-build-post-install": "node post-install.js"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "globals": {
      "__DEV__": true
    },
    "projects": [
      {
        "displayName": "web",
        "preset": "jest-expo/web",
        "testEnvironment": "jsdom",
        "testMatch": [
          "**/__tests__/web/**/*.tsx"
        ],
        "moduleFileExtensions": [
          "web.tsx",
          "web.js",
          "ts",
          "tsx",
          "js",
          "jsx",
          "json",
          "node"
        ],
        "setupFilesAfterEnv": [
          "./jest.setup.web.ts"
        ]
      },
      {
        "displayName": "native",
        "preset": "jest-expo",
        "testEnvironment": "jest-environment-node",
        "testMatch": [
          "**/__tests__/native/**/*.tsx"
        ],
        "moduleFileExtensions": [
          "ts",
          "tsx",
          "js",
          "jsx",
          "json",
          "node"
        ],
        "setupFilesAfterEnv": [
          "./jest.setup.ts"
        ]
      }
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|stream-chat-expo)"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/nunito-sans": "^0.2.3",
    "@expo/config-plugins": "^7.8.0",
    "@expo/webpack-config": "^19.0.0",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-community/geolocation": "^3.2.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/app": "20.0.0",
    "@react-native-firebase/auth": "20.0.0",
    "@react-native-firebase/crashlytics": "20.0.0",
    "@react-native-firebase/firestore": "20.0.0",
    "@react-native-firebase/functions": "20.0.0",
    "@react-native-firebase/messaging": "^20.0.0",
    "@react-native-firebase/storage": "20.0.0",
    "@react-native-google-signin/google-signin": "^10.1.1",
    "@react-native-masked-view/masked-view": "0.3.0",
    "@stream-io/flat-list-mvcp": "^0.10.3",
    "@tamagui/animations-react-native": "^1.61.3",
    "@tamagui/babel-plugin": "^1.61.3",
    "@tamagui/config": "^1.61.3",
    "@tamagui/font-inter": "^1.61.3",
    "@tamagui/lucide-icons": "^1.61.3",
    "@tamagui/themes": "^1.61.3",
    "@types/firebase": "^3.2.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "expo": "^50.0.19",
    "expo-apple-authentication": "~6.3.0",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.6",
    "expo-dev-client": "~3.3.12",
    "expo-device": "~5.9.4",
    "expo-file-system": "~16.0.9",
    "expo-font": "~11.10.3",
    "expo-image-manipulator": "~11.8.0",
    "expo-image-picker": "~14.7.1",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-media-library": "~15.9.2",
    "expo-notifications": "~0.27.8",
    "expo-router": "~3.4.10",
