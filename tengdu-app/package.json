    "expo-splash-screen": "~0.26.5",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^10.8.1",
    "install": "^0.13.0",
    "jest-expo": "~50.0.4",
    "jest-fetch-mock": "^3.0.3",
    "punycode": "^2.3.1",
