{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "module": "ES2020",
    "baseUrl": "./",
    "paths": {
      "firebaseConfig": ["/firebaseConfig.tsx", "/firebaseConfig.web.tsx"],
      "AuthProvider": ["/AuthProvider.tsx", "/AuthProvider.web.tsx"],
      "useStorageState": ["/useStorageState.tsx", "/useStorageState.web.tsx"],
      "components": ["./components"],
      "hooks": ["./hooks"],
      "utils": ["./utils"],
      "providers": ["./providers"],
      "__mocks__": ["./__mocks__"],
      "firebaseFunctions": ["./firebaseFunctions"],
      "GLOBAL_VALUES": ["./GLOBAL_VALUES.js"]
    }
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "jest.setup.ts",
    "global.d.ts",
    "GLOBAL_VALUES.js"
  ]
}
